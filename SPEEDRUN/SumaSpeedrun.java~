import java.util.Scanner;
class SumaSpeedrun {
  public static void main(String[] args) {
    Scanner input = new Scanner(System.in);
    // Deklaracija
    int n, suma = 0;
    System.out.print("Unesite n: ");
    n = input.nextInt();
    for (int i = 0;i <= n;i++) {
      for (int c = 0; c <= i; c++) {
      suma = suma + 1 / c;
      }
    }
    System.out.println(suma);
  }
}