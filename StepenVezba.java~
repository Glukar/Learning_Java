import java.util.*;
class StepenVezba {
  public static void main(String[] args) {
    Scanner input = new Scanner(System.in);
    // Deklaracija
    int x, n, rez;
    System.out.print("Unesite realan broj: ");
    x = input.nextInt();
    System.out.print("Unesite prirodan broj: ");
    n = input.nextInt();
    for (int i; i <= n; i++) {
      rez = x * x;
    }
    System.out.println("Vas rezultat je: " + rez);
  }
}